{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "space-before-function-paren": 0,
    "react/no-unknown-property": 0,
    "react/prop-types": "off",
    "multiline-ternary": 0,
    "no-new": 1
  },
  "settings": { "react": { "version": "detect" } }
}
